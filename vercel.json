{
  "version": 2,
  "name": "vkr",
  "builds": [
    { "src": "public/(.*)", "use": "@vercel/static-build" },
    { "src": "build/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/login",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "build/index.js"
    }
  ]
}
